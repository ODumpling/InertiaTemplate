{
  "$schema": "http://json.schemastore.org/template",
  "author": "ODumpling",
  "classifications": [
    "Web",
    "React",
    "Typescript"
  ],
  "name": "Inertia React Template",
  "description": "A project template for creating a web app with ASP.NET MVC and Inertiajs. Provides a starting point for Inertiajs apps using React and Typescript.",
  "shortName": "i-react",
  "sourceName": "InertiaTemplate.WebUI",
  "identity": "InertiaTemplate.WebUI",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        ".template.config/**/*",
        "templates/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "*.nuspec"
      ]
    }
  ]
}
    